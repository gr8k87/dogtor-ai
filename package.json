{
  "name": "dogtor-ai",
  "private": true,
  "scripts": {
    "dev": "npm run build:client && concurrently \"npm run dev --prefix server\" \"cd client && npm start\"",
    "build:client": "cd client && npm run build",
    "build:copy": "npm run build:client && rm -rf server/public && cp -r client/build server/public",
    "build": "npm run build:copy",
    "start": "cd server && npm start",
    "vercel-build": "npm run build:copy && npm install --prefix server"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.57.4",
    "@types/memoizee": "^0.4.12",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "connect-pg-simple": "^10.0.0",
    "express-session": "^1.18.2",
    "lucide-react": "^0.541.0",
    "memoizee": "^0.4.17",
    "openid-client": "^6.6.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "react-router-dom": "^7.8.2",
    "react-scripts": "^5.0.1",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "typescript": "^4.9.5"
  }
